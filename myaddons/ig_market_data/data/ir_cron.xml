<odoo>
  <record id="ir_cron_ig_asia_compute" model="ir.cron">
    <field name="name">IG Asia Session Compute</field>
    <field name="model_id" ref="model_ig_instrument"/>
    <field name="state">code</field>
    <field name="code">model.env["ig.asia.session"].action_compute_for_date()</field>
    <field name="interval_number">1</field>
    <field name="interval_type">days</field>
    <field name="numbercall">-1</field>
    <field name="active">True</field>
  </record>
</odoo>
