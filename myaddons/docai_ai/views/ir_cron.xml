<odoo>
    <data noupdate="1">
        <record id="ir_cron_docai_auto_import" model="ir.cron">
            <field name="name">Import factures PDF - Docai</field>
            <field name="model_id" ref="docai_ai.model_docai_ai_invoice_import"/>
            <field name="state">code</field>
            <field name="code">model.import_pdf_invoices()</field>
            <field name="interval_type">minutes</field>
            <field name="interval_number">10</field>
            <field name="active">True</field>
        </record>
    </data>

    <!-- CRON : Analyse automatique des factures avec DocAI -->
    <record id="ir_cron_docai_analyze_invoices" model="ir.cron">
        <field name="name">Analyse DocAI des factures</field>
        <field name="model_id" ref="account.model_account_move"/>
        <field name="state">code</field>
        <field name="code">model.cron_docai_analyze_invoices()</field>
        <field name="interval_number">5</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="active">True</field>
        <field name="nextcall" eval="(datetime.datetime.now() + datetime.timedelta(minutes=5)).strftime('%Y-%m-%d %H:%M:%S')"/>
    </record>


</odoo>
