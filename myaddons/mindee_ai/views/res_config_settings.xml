<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
  
        <record id="view_res_config_settings_inherit_mindee" model="ir.ui.view">
            <field name="name">res.config.settings.inherit.mindee</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="base_setup.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form" position="inside">

                        <block title="Clé API Mindee" groups="base.group_no_one" name="Mindee">
                            <setting id="Clé API Mindee" >
                              <field name="mindee_api_key" string="Clé API Mindee"/>
                            </setting>
                        </block>

                 
                </xpath>
            </field>
        </record>
    </data>
</odoo>

   
